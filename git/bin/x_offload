offload(){
    # moving a dir in current dir to dev and link it back
    # subdir under ~/.e is prefixed when moving to dev
    FR=$1
    RCWD=${PWD##/Users/jieli/.e/}
    TO=~/dev/$RCWD
    echo $FR $TO
    mkdir -p $TO 
    chmod -R u+rwx $FR
    mv $FR $TO
    ln -s $TO/$FR $FR
}

offload $1 $2

