	"Tasks":{
		"scope": "markdown",
		"prefix": "tasks",
		"body": [
			"# Tasks",
			"Task | Repeat | Date Init | Date Due | Status | Notes",
			"---|---|---|---|---|---",
			"dothis | m | ${CURRENT_YEAR}${CURRENT_MONTH}${CURRENT_DATE} |  | o |", 
			"|||||"
		],
		"description": "repeat(n, d, w, m, y);status(o, c, p)(detail in notes)" 	
	},		

	"Insurance":{
		"scope": "markdown",
